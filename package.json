{
  "name": "portal-administrativo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration desenv",
    "build_alpha": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "shx rm -rf coverage && ng test --watch=false --sourceMap=true --browsers=ChromeHeadlessNoSandbox --codeCoverage=true",
    "lint": "ng lint",
    "build:desenv": "ng lint && ng build --configuration desenv",
    "build:homol": "ng lint && ng build --configuration homol",
    "build:prod": "ng build --configuration prod",
    "pre-push": "npm run lint && npm run test",
    "angular-builds": "ng build --configuration prod",
    "postinstall": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~12.1.1",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "angularx-qrcode": "^13.0.15",
    "cpf-cnpj-validator": "^1.0.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "ng-recaptcha": "^9.0.0",
    "ngx-currency": "^2.5.3",
    "ngx-google-analytics": "^13.0.1",
    "ngx-file-drop": "^11.0.0",
    "ngx-mask": "^12.0.0",
    "ngx-mat-select-search": "^4.1.2",
    "rxjs": "~6.6.0",
    "shx": "^0.3.3",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular-eslint/builder": "^12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "^12.6.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.32.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "tslint-angular": "^3.0.3",
    "typescript": "~4.3.2",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "husky": "^7.0.2"
  }
}
